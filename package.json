{
  "name": "@fuzzysaj/location-to-usa-county",
  "version": "1.0.1",
  "scripts": {
    "prepare": "tsc",
    "cmd-line-lookup": "DEBUG=location-to-usa-county ts-node src/cmd-line-lookup.ts",
    "pretest": "tsc -p .",
    "test": "DEBUG=location-to-usa-county nyc mocha",
    "posttest": "nyc report --reporter=json"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": false
  },
  "license": "GPL-3.0",
  "repository": "fuzzysaj/location-to-usa-county",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "fuzzysaj",
  "dependencies": {
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "geojson-geometries-lookup": "^0.3.0",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@turf/helpers": "^6.1.4",
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "codecov": "^3.6.5",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
